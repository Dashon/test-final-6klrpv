{
  "name": "AI Travel Platform",
  "short_name": "AI Travel",
  "description": "AI-Enhanced Social Travel Platform for personalized travel planning and booking",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#FFFFFF",
  "theme_color": "#1A73E8",
  "orientation": "portrait",
  "scope": "/",
  "icons": [
    {
      "src": "/assets/images/logo-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/assets/images/logo-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View your travel dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/assets/images/dashboard-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Chat",
      "short_name": "Chat",
      "description": "Access travel chat interface",
      "url": "/chat",
      "icons": [
        {
          "src": "/assets/images/chat-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "categories": [
    "travel",
    "social",
    "productivity"
  ],
  "lang": "en-US",
  "dir": "ltr",
  "display_override": [
    "standalone",
    "minimal-ui",
    "browser"
  ],
  "screenshots": [
    {
      "src": "/assets/images/screenshots/desktop-1.png",
      "sizes": "1280x800",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard View"
    },
    {
      "src": "/assets/images/screenshots/mobile-1.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile Dashboard"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "id": "ai-travel-platform",
  "iarc_rating_id": "pending",
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "video/*"]
        }
      ]
    }
  }
}