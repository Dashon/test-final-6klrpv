{
  "name": "ai-travel-platform-android",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "android": "react-native run-android",
    "start": "react-native start",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "e2e:build": "detox build --configuration android.emu.release",
    "e2e:test": "detox test --configuration android.emu.release",
    "clean": "cd android && ./gradlew clean",
    "type-check": "tsc --noEmit",
    "security-scan": "npm audit && snyk test",
    "precommit": "lint-staged",
    "postinstall": "npx jetify",
    "validate": "npm-run-all lint type-check test",
    "metro:clean": "watchman watch-del-all && rm -rf $TMPDIR/metro-*"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-biometrics/auth": "^2.0.0",
    "@react-native-community/hooks": "2.x",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-community/safetynet": "^1.1.0",
    "@react-native-community/secure-store": "^1.0.0",
    "@react-native-community/spinner": "^1.3.0",
    "@react-native-firebase/analytics": "^18.0.0",
    "@react-native-firebase/crashlytics": "^18.3.0",
    "@react-native-firebase/perf": "^18.3.0",
    "@react-navigation/native": "6.x",
    "@react-navigation/stack": "^6.0.0",
    "@reduxjs/toolkit": "1.9.5",
    "@segment/analytics-react-native": "^2.13.0",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "date-fns": "^2.30.0",
    "http-status-codes": "^2.2.0",
    "jwt-decode": "^3.1.2",
    "react": "18.2.0",
    "react-error-boundary": "4.0.x",
    "react-native": "0.71.8",
    "react-native-biometrics": "2.x",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-haptic-feedback": "1.x",
    "react-native-reanimated": "^2.14.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-redux": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "socket.io-client": "4.6.1",
    "validator": "^13.9.0",
    "victory-native": "36.x"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@jest/globals": "29.5.x",
    "@testing-library/jest-dom": "5.16.x",
    "@testing-library/react-native": "^11.5.0",
    "@types/react": "18.0.28",
    "@types/react-native": "0.71.3",
    "axe-react-native": "^4.0.0",
    "axios-mock-adapter": "^1.21.0",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "18.x",
    "fastlane": "2.212.2",
    "fastlane-plugin-firebase_app_distribution": "0.4.2",
    "fastlane-plugin-match_keystore": "0.3.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "nock": "^13.3.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "4.9.5"
  }
}