{
  "name": "ai-travel-platform-backend",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "api-gateway",
    "booking-service",
    "ml-service",
    "persona-service",
    "professional-service",
    "social-service",
    "shared"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "turbo run dev",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,yml}\"",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "clean": "rimraf **/dist **/.turbo node_modules",
    "audit:fix": "npm audit fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/bull": "^9.0.0",
    "@nestjs/common": "9.0.x",
    "@nestjs/config": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^9.0.0",
    "@nestjs/swagger": "6.x",
    "@nestjs/terminus": "^9.0.0",
    "@nestjs/testing": "9.0.x",
    "@nestjs/throttler": "4.x",
    "@nestjs/typeorm": "9.0.x",
    "@socket.io/redis-adapter": "8.2.1",
    "@tensorflow/tfjs-node": "^4.8.0",
    "amadeus": "8.1.x",
    "axios": "1.4.x",
    "bcrypt": "5.1.0",
    "bull": "4.10.x",
    "class-transformer": "0.5.x",
    "class-validator": "0.14.x",
    "compression": "^1.7.4",
    "cors": "2.8.x",
    "dotenv": "16.3.x",
    "express": "4.18.x",
    "express-cache-controller": "1.1.x",
    "express-correlation-id": "2.0.x",
    "express-rate-limit": "6.7.x",
    "express-validator": "7.0.x",
    "fastapi": "0.95.x",
    "helmet": "7.0.x",
    "inversify": "6.0.x",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "9.0.x",
    "memwatch-next": "0.3.0",
    "mongoose": "^6.0.0",
    "nanoid": "4.0.x",
    "pg": "8.11.x",
    "prom-client": "14.2.0",
    "prometheus-client": "0.17.x",
    "python-dotenv": "1.0.x",
    "pyyaml": "6.0.x",
    "rate-limiter-flexible": "2.4.x",
    "redis": "7.0.11-alpine",
    "retry": "0.13.x",
    "rxjs": "7.8.x",
    "socket.io": "4.6.x",
    "socket.io-auth": "1.0.0",
    "socket.io-rate-limiter": "1.0.x",
    "socket.io-ratelimiting": "3.0.0",
    "stripe": "^11.0.0",
    "twilio": "4.x",
    "typedi": "0.10.x",
    "typeorm": "0.3.x",
    "typeorm-encrypted": "0.8.x",
    "typeorm-typedi-extensions": "0.4.x",
    "uvicorn": "0.22.x",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "4.7.x"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "29.5.x",
    "@types/jest": "29.x",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^5.59.x",
    "@typescript-eslint/parser": "^5.59.x",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.x",
    "eslint-plugin-import": "^2.27.x",
    "husky": "^8.0.3",
    "jest": "29.5.x",
    "lint-staged": "^13.2.0",
    "markdown-toc": "1.2.0",
    "mongodb-memory-server": "^8.12.0",
    "nodemon": "2.0.22",
    "performance-now": "^2.1.0",
    "prettier": "^2.8.x",
    "rimraf": "^4.4.0",
    "socket.io-mock": "^1.3.2",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.x",
    "ts-node": "10.9.1",
    "turbo": "^1.8.3",
    "typescript": "4.9.x"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test"
    }
  },
  "author": "AI Travel Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/organization/ai-travel-platform.git"
  }
}